{
  "name": "scriptlist-frontend",
  "private": true,
  "description": "",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "start": "remix-serve build",
    "light-css": "./node_modules/less/bin/lessc --js --modify-var=\"ant-prefix=light\"  styles/light.less app/styles/light.css",
    "dark-css": "./node_modules/less/bin/lessc --js --modify-var=\"ant-prefix=dark\" styles/dark.less app/styles/dark.css"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@types/ua-parser-js": "^0.7.36",
    "antd": "^4.20.4",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "github-markdown-css": "^5.1.0",
    "marked": "^4.0.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@types/marked": "^4.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "less": "^4.1.2",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
